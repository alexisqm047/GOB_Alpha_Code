@page
@model GOB.AppFrontend.Pages.PersonalModel
@{
}
<style>
h1 {text-align: center;}
p {text-align: center;}
div {text-align: center;}
</style>
<style>
table, tr, th, td {
  border: 1px solid white;
  border-collapse: collapse;
}
th, td {
    background-color: #96D4D4;
}
</style>
<h1>Lista Personal de Aseo</h1>
<table style="width:100%"> 
        <th>Nombre</th>
        <th>Apellido</th>
        <th>Edad</th>
        <th>Identificacion</th>
        <th>Turno</th>
        <th>Estado de salud</th>
        @foreach (var empleadosAseo in Model.Personal)
        {   

                <tr>
                        <td>@empleadosAseo.nombre</td>
                        <td>@empleadosAseo.apellido</td>
                        <td>@empleadosAseo.edad</td>
                        <td>@empleadosAseo.identificacion</td>
                        <td>@empleadosAseo.turno</td>
                        <td>@empleadosAseo.estadoSalud</td>
                </tr>
                
        }
</table>

