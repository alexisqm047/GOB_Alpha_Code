@page
@model GOB.AppFrontend.Pages.EditarPersonalModel
@{
}
<form method="post">
    <input type="hidden" asp-for="personal.id">
    <div class="form_group">
        <label asp-for="personal.nombre"></label>
        <input asp-for="personal.nombre" class="form-control">
    </div>
    <div class="form_group">
        <label asp-for="personal.apellido"></label>
        <input asp-for="personal.apellido" class="form-control">
    </div>
    <div class="form_group">
        <label asp-for="personal.edad"></label>
        <input asp-for="personal.edad" class="form-control">
    </div>
    <div class="form_group">
        <label asp-for="personal.identificacion"></label>
        <input asp-for="personal.identificacion" class="form-control">
    </div>
    <div class="form_group">
        <label asp-for="personal.turno"></label>
        <input asp-for="personal.turno" class="form-control">
    </div>
    <div class="form_group">
        <label asp-for="personal.estadoSalud"></label>
        <input asp-for="personal.estadoSalud" class="form-control">
    </div>
    <button type="submit" class="btn btn-primary">Guardar Cambios</button>
    <input type="button" class="btn btn-primary" onclick="history.back()" name="Cancelar" value="Cancelar">
</form>